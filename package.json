{
  "name": "@riktikdev/ts-starter",
  "version": "0.0.1",
  "packageManager": "yarn@4.9.2",
  "description": "A typescript template for projects",
  "author": "riktikdev <github.com/riktikdev>",
  "license": "MIT",
  "homepage": "https://github.com/riktikdev/ts-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riktikdev/ts-starter.git"
  },
  "bugs": "https://github.com/riktikdev/ts-starter/issues",
  "keywords": [
    "ts",
    "typescript",
    "nodejs",
    "node",
    "template",
    "typescript template"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "start": "cross-env NODE_ENV=production node -r tsconfig-paths/register ./dist/index",
    "dev": "nodemon --config ./nodemon.json",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "pretty": "eslint . --fix && prettier --write .",
    "prepare": "husky"
  },
  "dependencies": {
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@eslint/js": "^9.31.0",
    "@types/node": "22",
    "bumpp": "^10.2.0",
    "eslint": "^9.31.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  }
}
